{
  "name": "template-tauri-react",
  "version": "0.0.1",
  "description": "A modern cross-platform desktop application template built with Tauri 2.0, React 18, TypeScript, and Ant Design. Features system monitoring, 3D rendering, animation system, and comprehensive development tools.",
  "type": "module",
  "private": false,
  "keywords": [
    "tauri",
    "react",
    "typescript",
    "desktop-app",
    "cross-platform",
    "rust",
    "vite",
    "ant-design",
    "unocss",
    "three.js",
    "3D",
    "zustand",
    "swr",
    "template",
    "boilerplate"
  ],
  "author": {
    "name": "PureSilent",
    "email": "puresilent.com@gmail.com",
    "url": "https://github.com/mobaibai"
  },
  "license": "MIT",
  "homepage": "https://github.com/mobaibai/template-tauri-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mobaibai/template-tauri-react.git"
  },
  "bugs": {
    "url": "https://github.com/mobaibai/template-tauri-react/issues"
  },
  "engines": {
    "node": ">=22.17.0",
    "pnpm": ">=10.0.0"
  },
  "main": "dist/index.html",
  "files": ["dist", "src-tauri/target/release/bundle", "README.md", "LICENSE"],
  "scripts": {
    "========= 启动前端开发服务器(Tauri项目会连带执行不需要单独执行) =========": "",
    "dev": "vite",
    "========= 构建前端应用(Tauri项目会连带执行不需要单独执行) =========": "",
    "build": "tsc && vite build",
    "========= 预览构建后的前端应用 =========": "",
    "preview": "vite preview",
    "========= 启动Tauri桌面应用程序开发模式 =========": "",
    "dev:tauri": "tauri dev",
    "========= 启动iOS应用程序开发模式 =========": "",
    "dev:ios": "tauri ios dev",
    "========= 安全启动iOS开发模式(包含环境检查) =========": "",
    "dev:ios:safe": "npm run setup:ios && npm run dev:ios",
    "========= 启动Android应用程序开发模式 =========": "",
    "dev:android": "tauri android dev",
    "========= 安全启动Android开发模式(包含环境检查) =========": "",
    "dev:android:safe": "npm run setup:android && npm run dev:android",
    "========= 初始化iOS项目 =========": "",
    "init:ios": "tauri ios init",
    "========= 初始化Android项目 =========": "",
    "init:android": "tauri android init",
    "========= 构建Tauri默认桌面应用程序 =========": "",
    "build:tauri": "tauri build",
    "========= 构建macOS x86版本 =========": "",
    "build:mac-x86": "node scripts/build.js mac-x86",
    "========= 构建macOS ARM64版本 =========": "",
    "build:mac-aarch": "node scripts/build.js mac-aarch",
    "========= 构建Windows x86版本 =========": "",
    "build:win-x86": "node scripts/build.js win-x86",
    "========= 构建Windows ARM64版本 =========": "",
    "build:win-aarch": "node scripts/build.js win-aarch",
    "========= 构建Linux x86版本 =========": "",
    "build:linux-x86": "node scripts/build.js linux-x86",
    "========= 构建Linux ARM64版本 =========": "",
    "build:linux-aarch": "node scripts/build.js linux-aarch",
    "========= 构建所有桌面平台版本(不包含Linux) =========": "",
    "build:desktop": "node scripts/build.js desktop",
    "========= 构建iOS应用 =========": "",
    "build:ios": "node scripts/build.js ios",
    "========= 构建iOS模拟器版本 =========": "",
    "build:ios-sim": "node scripts/build.js ios-sim",
    "========= 构建Android应用 ==========": "",
    "build:android": "node scripts/build.js android",
    "========= 构建Android x86版本 ==========": "",
    "build:android-x86": "node scripts/build.js android-x86",
    "========= 对Android APK进行调试签名 ==========": "",
    "sign:android": "node scripts/sign-apk.js",
    "========= 构建所有移动平台版本 =========": "",
    "build:mobile": "node scripts/build.js mobile",
    "========= 构建所有平台版本 =========": "",
    "build:all": "node scripts/build.js all",
    "========= 检查开发环境配置 ==========": "",
    "check:env": "node scripts/check-env.js",
    "========= 检查并自动修复开发环境配置 ==========": "",
    "check:env:fix": "node scripts/check-env.js --fix",
    "========= 检查 Node.js 和 pnpm 版本一致性 ==========": "",
    "check:versions": "node scripts/check-node-version.js",
    "========= 修复OpenSSL配置问题 ==========": "",
    "fix:openssl": "node scripts/fix-openssl.js",
    "========= 设置Android开发环境 =========": "",
    "setup:android": "npm run check:env && npm run emulator:start",
    "========= 设置iOS开发环境 =========": "",
    "setup:ios": "npm run check:env && npm run simulator:start",
    "========= 查看Android模拟器状态 =========": "",
    "emulator:status": "node scripts/android-emulator.js status",
    "========= 启动Android模拟器 =========": "",
    "emulator:start": "node scripts/android-emulator.js start",
    "========= 停止所有Android模拟器 =========": "",
    "emulator:stop": "node scripts/android-emulator.js stop",
    "========= 交互式选择并启动Android模拟器 =========": "",
    "emulator:interactive": "node scripts/android-emulator.js interactive",
    "========= 查看iOS模拟器状态 =========": "",
    "simulator:status": "xcrun simctl list devices",
    "========= 启动iOS模拟器 =========": "",
    "simulator:start": "node scripts/ios-simulator.js start",
    "========= 停止所有iOS模拟器 =========": "",
    "simulator:stop": "xcrun simctl shutdown all",
    "========= 交互式选择并启动iOS模拟器 =========": "",
    "simulator:interactive": "node scripts/ios-simulator.js interactive",
    "========= 清理构建产物和缓存 ==========": "",
    "clean": "rm -rf dist dist-builds node_modules/.vite",
    "========= 清理构建输出目录 ==========": "",
    "clean:dist": "rm -rf dist-builds && mkdir -p dist-builds",
    "========= 清理并重新安装依赖 ==========": "",
    "clean:deps": "rm -rf node_modules package-lock.json yarn.lock && npm install",
    "========= 清理跨平台构建缓存 ===========": "",
    "clean:build-cache": "node scripts/cleanup-build-cache.js",
    "========= 清理整个构建缓存目录 ===========": "",
    "clean:build-temp": "rm -rf src-tauri/target/*",
    "========= 执行全面清理(包括Rust缓存) ===========": "",
    "clean:all": "npm run clean && npm run clean:deps && cd src-tauri && cargo clean",
    "========= 运行ESLint代码检查 =========": "",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "========= 运行ESLint并自动修复问题 =========": "",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "========= 运行TypeScript类型检查 =========": "",
    "type-check": "tsc --noEmit",
    "========= 格式化代码 =========": "",
    "format": "prettier --write .",
    "========= 检查代码格式 =========": "",
    "format:check": "prettier --check .",
    "========= 格式化Rust代码 =========": "",
    "format:rust": "cd src-tauri && cargo fmt",
    "========= 检查Rust代码格式 =========": "",
    "format:rust:check": "cd src-tauri && cargo fmt --check",
    "========= 运行Rust测试 =========": "",
    "test:rust": "cd src-tauri && cargo test",
    "========= 运行Rust Clippy代码检查 =========": "",
    "clippy": "cd src-tauri && cargo clippy -- -D warnings",
    "========= 提交前代码检查和格式化 =========": "",
    "precommit": "npm run lint:fix && npm run format && npm run type-check",
    "========= 构建前检查 =========": "",
    "prebuild": "npm run type-check && npm run format:check",
    "========= 安装后获取Rust依赖 =========": "",
    "postinstall": "cd src-tauri && cargo fetch"
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@react-spring/web": "^10.0.1",
    "@react-three/drei": "^10.4.4",
    "@react-three/fiber": "^9.2.0",
    "@tauri-apps/api": "^2.6.0",
    "@tauri-apps/plugin-opener": "^2.4.0",
    "antd": "^5.26.3",
    "axios": "^1.10.0",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "swr": "^2.3.4",
    "three": "^0.178.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@iconify/json": "^2.2.355",
    "@tauri-apps/cli": "^2.6.2",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/crypto-js": "^4.2.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/three": "^0.178.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "prettier": "^3.6.2",
    "sass": "^1.89.2",
    "svgo": "^4.0.0",
    "svgstore": "^3.0.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.20.0",
    "unocss": "^66.3.2",
    "vite": "^7.0.2"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  },
  "browserslist": {
    "production": [">0.2%", "not dead", "not op_mini all"],
    "development": ["last 1 chrome version", "last 1 firefox version", "last 1 safari version"]
  }
}
